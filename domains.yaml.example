# domains.yaml
domains:
  # 云资源类型
  - domain: "example.com"
    deploy_type: "cloud"
    resource_id: 256644

  # 服务器类型 1
  - domain: "api.example.com"
    deploy_type: "server"
    cert_path: "/etc/nginx/certs/mysite/"

  # 服务器类型 2
  - domain: "doc.example.com"
    deploy_type: "server"
    cert_path: "/var/www/blog/ssl/"
